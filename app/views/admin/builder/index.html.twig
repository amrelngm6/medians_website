				function kt_Load(data, opt) {
					//alert(data)

					//i

					if (data != "") {

						// reset menu settings to default first...
						jQuery('#kt__Settings .btn-group-toggle label').removeClass('active');
						jQuery('#kt__Settings input[type=checkbox]').prop('checked', false);
						jQuery('#kt__Settings input[type=text]').val('0');
						jQuery('#kt__Settings input.colorPicker').val('').attr('style', null);

						jQuery('#kt__Settings textarea').val('');

						//if(typeof data==="string")

						var kt__data = JSON.parse(data);
						//else kt__data=data;

						//kt__Settings
						//jQuery('#kt__logoBlock,#kt__itemShadow,#kt__itemCapital,#kt__itemTransition,#kt__shadow,#kt__fixed').prop('checked',false);
						//jQuery('#kt__customCSS').val('');

						for (i in kt__data) {

							var theId = 'kt__' + i;
							// if it's just a text input, set new value.
							if (typeof kt__data[i].t == 'undefined') { // this is probaby a normal text input

								jQuery('#' + theId).val(kt__data[i].v);

								// if the value is "!", it's probably a spinner with more-details enabled.
								if (kt__data[i].v == '!') {

									jQuery('#' + theId).closest('.spinnersLess').hide().next().show();

								} else {

									var attr = jQuery(this).attr('data-custom');
									if (typeof attr !== typeof undefined && attr !== false) {
										// Element has this attribute
									} else {

										jQuery('#' + theId).closest('.spinnersLess').show().next().hide();

									}

								}

								// if it's just a text input, set new value.
							} else if (kt__data[i].t == 'c' /*checkbox*/ || kt__data[i].t == 'r' /*radio*/ ) {
								//if(kt__data[i].type == 'radio')
								//alert(kt__data[i].type + '/ ' +typeof kt__data[i].value + '/'+kt__data[i].value)

								if (typeof kt__data[i].v !== "undefined" && kt__data[i].v != "") {
									//alert('setting '+kt__data[i].type+' ('+i+') to Checked.')
									jQuery('#' + theId).prop('checked', true);

									// now additionally if its parent is Label with btn class, set it to active.
									if (jQuery('#' + theId).parent().hasClass('btn'))
										jQuery('#' + theId).parent().addClass('active');

								} else {

									jQuery('#' + theId).prop('checked', false);
								}

							}

						}
						if (opt == 1) {

							kt_Generate();

						} else if (opt == 2) {

							// save it to file.
							kt_Generate(2);

						}
					}

					jQuery('input[name=kt__menuType]').trigger("change");

				}
				function kt_Save() {

					var kt__data = {};
					jQuery('#kt__Settings input,#kt__Settings select,#kt__customCSS').each(function() {

						var $vid = jQuery(this).attr('id');
						var $vtag = jQuery(this).prop('nodeName');

						if ($vid == 'kt__customCSS') {
							var $vtype = 'text';
							$vtag = 'INPUT';
						} else
							var $vtype = jQuery(this).attr('type');
						if ($vtype == 'checkbox')
							var $vval = jQuery('#' + $vid + ':checked').val();
						else if ($vtype == 'radio')
							var $vval = jQuery('#' + $vid + ':checked').val();
						else
							var $vval = jQuery(this).val();
						if(typeof $vval!=="undefined")
							$vval=$vval.replace(/(<([^>]+)>)/gi, "");

						if ($vtype == 'text' && $vtag == 'INPUT') {

							$vval = $vval.replace(/\n/g, " ");
							$vval = encodeURIComponent($vval);
							kt__data[$vid.substring(4)] = {

								v: $vval

							}

						} else {

							kt__data[$vid.substring(4)] = {

								t: $vtype[0],
								v: $vval

							}
						}

					})
					return kt__data;

				}
