				function updateNewKeditItem(value, opt, contentId) {

					// if there's contentEditable toolbar opened, close it
					keditableToolbar('hide');

					if (opt == 'cancel') {

						// remove new item.

						jQuery("a#newKeditItem").remove(); // old .WxEditableArea a#newKeditItem
						jQuery(".keditItem").remove(); // old .WxEditableArea .keditItem
						return;

					}

					var valueBegin = value.substr(0, 2);
					// OLD: just replace user's dragged item with "kedit" element, module's tag as content,
					// it'll now work only for normal content: only if first 2 letters are not module's tag start
					var kedit_before = '';
					var kedit_after = '';
					var kedit_refresh = 0;

					var keditDragHelper = jQuery('.keditItem'); // old: '.WxEditableArea .keditItem'

					if (keditDragHelper.length === 0) return;

					if (valueBegin != '{@') {

						value = kedit_before + value + kedit_after;

						if (value.indexOf("newKeditItem") < 0){

							// does the block come from block designer?

							if(typeof keditBlockMaker_el === "number"){

								value = '<section id="newKeditItem" data-keditor="1" class="kedit">' + value + '</section>';
								keditBlockMaker_el=[];

							}else{
								value = '<section id="newKeditItem" class="kedit"><div class="w-full">' + value + '</div></section>';
							}

						}
							

						// is it FOOTER?
						var contentId = parseFloat(contentId);
						if (contentId > 5500 && contentId < 5501) {

							// we're replacing footer.
							/*

							we'll try to not lose too much user's data when doing this migration, so these items will be copied:
							IMG.keditFooterLogo
							.keditFooterCompany
							.keditFooterCopyright
							.keditFooterMenu {@sitemap@}
							.keditFooterApp {@footer@}

							*/

							keditDragHelper.html(value);
							keditDragHelper.find('.keditFooterCompany').html(jQuery("#footerContent .keditFooterCompany").html());
							keditDragHelper.find('.keditFooterLogo').attr('src', jQuery("#footerContent .keditFooterLogo").attr('src'));
							keditDragHelper.find('.keditFooterCopyright').html(jQuery("#footerContent .keditFooterCopyright").html());

							var theFooter = jQuery("#footerContent");
							theFooter.html(keditDragHelper.html());
							
							
							keditDragHelper.remove();
							k_EditSave();
							

						} else {

							keditDragHelper.replaceWith(value);

						}

					} else {
						var sectionClass='kedit keditApp noedit',sectionSettings='',containerBefore='<div class="w-full">',containerAfter='</div>';

						if(value == '{@cal@}' || value == '{@calendly@}'){

							sectionClass='kedit keditApp keditFullHeight';
							sectionSettings=' style="padding-top:0px;padding-bottom:0px;" data-padding="0-0"';					containerBefore='';//<div class="no-container">';
							containerAfter='';

						}
						// NEW: just replace user's dragged item with "kedit" element,
						// module's tag is not visible, user will see only placeholder message, 
						// which allows him to click for save changes:

						jQuery(".keditItem").replaceWith('<section id="newKeditItem" class="'+sectionClass+'"'+sectionSettings+'>' + containerBefore + kedit_before + '<div class="keditPlaceholder">Hey! This module will be visible when you save changes<span>' + value + '</span>' + containerAfter + kedit_after + '</div></section>'); // old: '.WxEditableArea .keditItem'

					}
					// now hide toolbar.
					extraAdd('hide');

					//keditDraggable(newKeditItem,'noedit');return;

					if (opt == 'save') {

						// module is added, now save changes to refresh it.
						k_EditSave();

					} else if (opt == 'silentSave') {

						// FUNCTION IS NOT WORKING YET.
						// module is added, now save changes, but no refresh.
						k_EditSave('silent');

					} else {

						if (jQuery("#newKeditItem2").length > 0) {

							keditDraggable(jQuery("#newKeditItem2"));

							if (jQuery("#newKeditItem3").length > 0) {
								keditDraggable(jQuery("#newKeditItem3"));
								if (jQuery("#newKeditItem4").length > 0) {
									keditDraggable(jQuery("#newKeditItem4"));
								}
							}

						}

						keditDraggable(jQuery("#newKeditItem"), opt);

						if (typeof theFooter !== "undefined") {

							theFooter.find('.footer_koparsed').addClass('koparsed').removeClass('footer_koparsed');

