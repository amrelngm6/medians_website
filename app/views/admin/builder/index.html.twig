						var saveToTheme_ = {},menuStyleBar={},
							menuDesignPosition=jQuery('#headerMenu').css('position'),
							menuDesignPosition_direction=jQuery('#headerMenu').css('flex-direction'),
							menuDesignPosition_holder=jQuery('#header').css('position'),
							mb__backgroundColor=jQuery('#mb__backgroundColor').val(),
							mb__textColor=jQuery('#mb__textColor').val();

						menuStyleBar.close=jQuery('#mb__closeButton').prop('checked');
						menuStyleBar.social=jQuery('#mb__socialButtons').prop('checked');
						menuStyleBar.center=jQuery('#mb__centerItems').prop('checked');
						menuStyleBar.active=jQuery('#mb__showBanner').prop('checked');

						menuStyleBar.colors={

							'text':mb__textColor,
							'background':mb__backgroundColor

						};

						jQuery('#headerMenuBar').attr('data-colors',mb__textColor+'|'+mb__backgroundColor);

						

						menuStyleBar.content=window.btoa(encodeURIComponent(jQuery('#headerMenuBar_content').html()));

						// does current menu design require extra space on top?
						jQuery('html').removeClass('ko_ThemeOn ko_ThemeFixed ko_ThemeOnSide');
						
						
						if (jQuery('#mb__showBanner').prop('checked')) {

							// if it's  asidebar menu, menuDesignPosition_holder is absolute.
							if(menuDesignPosition == 'fixed'
							|| menuDesignPosition_holder == 'fixed'){

								jQuery('html').addClass('ko_ThemeFixed');
								menuStyleBar.space=2;

							} else if(menuDesignPosition == 'absolute'
							|| menuDesignPosition_holder == 'absolute'){

								jQuery('html').addClass('ko_ThemeOn');
								menuStyleBar.space=1;

							} else if(menuDesignPosition_direction == 'column' 
										/* sidebar menu */){

								

								jQuery('html').addClass('ko_ThemeOnSide');
								menuStyleBar.space=3;
							}

						}
						
						
						saveToTheme_.menuStyleBar = JSON.stringify(menuStyleBar);
						
						saveToThemeArray(saveToTheme_, 0, 1);
						koMenuLoading('saved');
					}
					function mb_Generate(){
						// make sure we're back in editing mode when color picker is closed
						jQuery('html').addClass('editingHeaderBanner');

						headerMenuBar_updateElements();

						var mb__backgroundColor=jQuery('#mb__backgroundColor').val(),
							mb__textColor=jQuery('#mb__textColor').val();

						jQuery('#headerMenuBar').attr('data-color',mb__textColor+'|'+mb__backgroundColor)

						if (mb__backgroundColor.indexOf('--color') > -1) 
							mb__backgroundColor='var('+mb__backgroundColor+')';
						if (mb__textColor.indexOf('--color') > -1) 
							mb__textColor='var('+mb__textColor+')';
						jQuery('#headerMenuBar').css({

							'--headerBanner_bg':mb__backgroundColor,
							'--headerBanner_text':mb__textColor
							
						})

						if (jQuery('#mb__closeButton').prop('checked')) {
							jQuery('.headerBanner_close').removeClass('d-none');
						}else{
							jQuery('.headerBanner_close').addClass('d-none');
						}

						if (jQuery('#mb__centerItems').prop('checked')) {
							jQuery('#headerMenuBar_content').addClass('justify-content-center');
						}else{
							jQuery('#headerMenuBar_content').removeClass('justify-content-center');
						}
						

						if (jQuery('#mb__showBanner').prop('checked')) {
							
							jQuery('#headerMenuBar').removeClass('d-none');
							jQuery('#koHeaderMenuBar').prop('checked',true); // this is a switch in DESIGN > MORE section
							

						}else{
							
							jQuery('#headerMenuBar').addClass('d-none');
							jQuery('#koHeaderMenuBar').prop('checked',false);

						}

						
						if (jQuery('#mb__socialButtons').prop('checked')) {
							jQuery('.headerBanner_social').removeClass('d-none');
						}else{
							jQuery('.headerBanner_social').addClass('d-none');
						}

						

					}
					jQuery('#mb__closeButton,#mb__showBanner,#mb__centerItems,#mb__socialButtons').on('input',mb_Generate)
