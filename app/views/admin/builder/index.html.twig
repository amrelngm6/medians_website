                        <script>
                            var picker, pickerAlphaValue = 1;

										function customColorPicker_Init(defaultColor) {
											pickerAlphaValue = 1;

											function _trim(str) {
												return str.replace(/^\s+|\s+$/gm, '');
											}

											function _rgbaToHex(rgba) {

												if (typeof rgba === "undefined" || rgba == "")
													return getComputedStyle(document.body).getPropertyValue('--color1'); //'var('+hexcolor+')';return '#0B5CEA';
												if (rgba.indexOf('var(') == 0) {
													rgba = rgba.split("(")[1].split(")")[0];
												}

												if (rgba.indexOf('--') == 0) {

													rgba = getComputedStyle(document.body).getPropertyValue(rgba); //'var('+hexcolor+')';
												}

												if (rgba.indexOf('#') == 0) return rgba;
												if (rgba.indexOf('(') < -1 && rgba.indexOf(')') < -1) return rgba;

												//rgba = rgba.split("(");							//alert(rgba);rgba=rgba[1];

												//rgba = rgba.split(")");rgba=rgba[0];

												rgba = rgba.split("(")[1].split(")")[0].split(',');
												//parseInt(rgb[1],10).toString(16)).slice(-2)

												//alert(rgba)
												let r = (+rgba[0]).toString(16),
													g = (+rgba[1]).toString(16),
													b = (+rgba[2]).toString(16);
												if (r.length == 1)
													r = "0" + r;
												if (g.length == 1)
													g = "0" + g;
												if (b.length == 1)
													b = "0" + b;
												//alert(rgba.length + ' / ' + rgba[3])
												if (rgba.length > 3)
													pickerAlphaValue = parseFloat(rgba[3]);
												return "#" + r + g + b;

											}
											var hexColor = jQuery('#customColorValue').val();

											if (hexColor.indexOf('--color') > -1) {
												if (hexColor.indexOf('var(') == 0) {
													hexColor = hexColor.split("(")[1].split(")")[0];
												}

												// it's variable
												hexColor = getComputedStyle(document.body).getPropertyValue(hexColor); //'var('+hexcolor+')';

											}

											hexColor = _rgbaToHex(hexColor);
											if (typeof picker !== "undefined")
												picker.destroy();

											var pickerHolder = jQuery('#colorPickerHolder');

											pickerHolder.html('');
											pickerHolder = pickerHolder[0];
											var pickerValue = jQuery('#customColorValue')[0],
												pickerAlpha = document.createElement('input');

											picker = new CP(pickerValue, false, pickerHolder);

											

											function pickerAlphaChange() {
												var v = CP._HSV2RGB(picker.set());
												v = this.value == 1 ? 'rgb(' + v.join(', ') + ')' : 'rgba(' + v.join(', ') + ', ' + this.value.replace(/^0\./, '.') + ')';

												if (this.value == 1) {
													v = '#' + CP._HSV2HEX(picker.get());
												}

												picker.source.value = v;
												//document.body.style.backgroundColor = v;
											}

											pickerAlpha.type = 'range';
											pickerAlpha.min = 0;
											pickerAlpha.max = 1;
											pickerAlpha.step = .01;
											pickerAlpha.value = pickerAlphaValue; // opaque
											pickerAlpha.onchange = pickerAlphaChange;
											pickerAlpha.oninput = pickerAlphaChange;
											pickerAlpha.className = 'formSlider';
											pickerAlpha.id = '_pickerAlpha';

											var pickerAlphaHolder = document.createElement('div');
											pickerAlphaHolder.className = '';

											picker.self.appendChild(pickerAlphaHolder);
											pickerAlphaHolder.appendChild(pickerAlpha);
											
											picker.fit = function() { // do nothing ...
												this.self.style.left = this.self.style.top = "";
											};

											picker.on("change", function(color) {

												this.source.value = '#' + color;
												cPreview('#' + color);
												var v = CP.HEX2RGB(color);
												v = pickerAlpha.value == 1 ? '#' + color
													:
													'rgba(' + v.join(', ') + ', ' + pickerAlpha.value.replace(/^0\./, '.') + ')';
												this.source.value = v;

											});

											function _update() {
												picker.set(this.value).enter();

												var reg=/^#([0-9a-f]{3}){1,2}$/i;
												if(reg.test(this.value)){

													cPreview(this.value);

												}

												
												
											}
											

											picker.source.oncut = _update;
											picker.source.onpaste = _update;
											picker.source.onkeyup = _update;
											picker.source.oninput = _update;
											// add a `static` class to the color picker panel
											picker.self.classList.add('static');
											// force to show the color picker panel
											picker.enter();

											picker.set(_rgbaToHex(defaultColor));

										}
									</script>
