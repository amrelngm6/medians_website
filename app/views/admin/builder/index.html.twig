				function koEmbedVideo(url, opt) {

					if (url.indexOf('vimeo.com/') > -1) {

						url = url.split('/');
						if (url.length > 0)
							url = url[url.length - 1];

						if (url > 0) {

							return ('//player.vimeo.com/video/' + url);

						}

					} else if (
						
					   url.indexOf('youtu.be/') > -1 
					|| url.indexOf('youtube.com/') > -1 
					|| url.indexOf('youtube-nocookie.com/') > -1) {

						var regExp = /^.*(youtu.be\/|v\/|e\/|u\/\w+\/|embed\/|v=)([^#\&\?]*).*/;
						var match = url.match(regExp);
						var videoId;

						if (match && match[2].length == 11) {
							videoId = match[2];
						} else {
							videoId = 0;
						}

						if (videoId == 0) {

							// return an error;
							alert('Oops! Your video address is incorrect');
							return false;

						} else {

							if (opt == 1) return '//www.youtube-nocookie.com/embed/' + videoId;

						}

					}
					alert('Oops! Your video address is incorrect');
					return false;

				}

