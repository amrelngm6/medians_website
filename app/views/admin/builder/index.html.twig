							};

							function koPageFonts(opt) {

								var font1 = jQuery("#ko_font1").val().replace(/\+/g, ' ');
								var font2 = jQuery("#ko_font2").val().replace(/\+/g, ' ');

								// split font into family and weight
								font1 = font1.split(':');
								font2 = font2.split(':');

								font1 = font1[0];
								font2 = font2[0];
								

								if(typeof systemFonts[font1]!=="undefined"){
									//gfont1='Poppins';
									font1=systemFonts[font1];
								}

								if(typeof systemFonts[font2]!=="undefined"){
									//gfont2='Poppins';
									font2=systemFonts[font2];
								}

								if(font1.indexOf(',') > 0)
								var gfont1='Poppins';
								else
								var gfont1=font1;

