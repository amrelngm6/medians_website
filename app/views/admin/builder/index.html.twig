				function refreshWindow(urlAddon) {

					var scrollBack = jQuery(window).scrollTop();

					if (urlAddon === 0 
					// change made in March 1, 2022 to make blog post refresh into the same window.
					|| typeof urlAddon === "undefined"
					) {

						koShowLoading();

						var location="/";

						if (scrollBack > 0) scrollBack = 'scroll-to=' + scrollBack;
						else scrollBack = '';

						if (location.indexOf("?") > 0)
							location += '&' + scrollBack;
						else
							location += '?' + scrollBack;
						if (typeof urlAddon !== 'undefined')
							location += '&' + urlAddon;
						location = trimChar(location, '?');
						location = trimChar(location, '&');
						window.location.href = location;
						return true;
					} else
						// before refreshing anything - is there "save changes" waiting?
						if (jQuery("#dragHelper").attr('rel') == 1 || refreshWithSave == 1) {
							jQuery("#dragHelper").attr('rel', '');
							k_EditSave();
							refreshWithSave = 0;

						} else {

							// nothing to save. Just refreshing.
							// is there are any modal windows opened, close them.
							koShowLoading();
							var location="/{{page.prefix}}";							
							if (scrollBack > 0) scrollBack = 'scroll-to=' + scrollBack;
							else scrollBack = '';

