    <script>
        function koToolbarTabs(i, ii, tab) {

				// hide frame if any was already opened.
				koHideFrame();
				jQuery("#koWelcome2").hide();

				if (i == 'Widget') {

					keditableToolbar('hide');

					var widgetApp = jQuery(ii).closest('[data-app]');
					var widgetAppType = widgetApp.attr('data-app');

					cAppId = widgetApp;

					// hide all control tabs and show only the one we'll be editing now
					jQuery('.kappControl').addClass('d-none');

					if (widgetAppType == 'instagram') {
						// default values:
						var kapp_instagram = {};

						kapp_instagram.margin = 1;
						kapp_instagram.username = 'instagram';
						kapp_instagram.items = '6';
						kapp_instagram.itemRows = '1';
						jQuery('#koMenu_widgetTitle').text('Instagram Feed');
						jQuery('#kapp_instagramUsername').val(widgetApp.attr('data-username'));
						
						if (typeof widgetApp.attr('data-margin') !== "undefined" &&
							widgetApp.attr('data-margin') == "0") {

							jQuery('#kapp_instagramMargin').prop('checked', false);

						} else
							jQuery('#kapp_instagramMargin').prop('checked', true);

						if (typeof widgetApp.attr('data-items') !== "undefined" &&
							parseFloat(widgetApp.attr('data-items')) > 0) {

							jQuery('#kapp_instagramItems').val(widgetApp.attr('data-items'));

						} else {
							jQuery('#kapp_instagramItems').val(kapp_instagram.items);
						}
						if (typeof widgetApp.attr('data-rows') !== "undefined" &&
							parseFloat(widgetApp.attr('data-rows')) > 0) {

							jQuery('#kapp_instagramItemsRow').val(widgetApp.attr('data-rows'));

						} else {

							jQuery('#kapp_instagramItemsRow').val(kapp_instagram.itemRows);

						}

						//kapp_instagramMargin
						//kapp_instagramItems
						//kapp_instagramItemsRow
						// now show instagram controls:
						jQuery('#kappControl_instagram').removeClass('d-none');
					} else if (widgetAppType == 'textSlider') {

						jQuery('#koMenu_widgetTitle').text('Text Slider');
					} else if (widgetAppType == 'countdown') {
						jQuery('#koMenu_widgetTitle').text(cbPhrases.labels.clickToAdd_objectCountdown);

						var countdownValue='',countdownValueTime='';

						if(typeof widgetApp.attr('data-value')!=="undefined"){
							countdownValue=widgetApp.attr('data-value');
						}

						if(countdownValue.indexOf('T')){

							countdownValue=countdownValue.split('T');
							countdownValueTime=countdownValue[1];
							countdownValue=countdownValue[0];

						}

						jQuery('#kapp_countdownDate').val(countdownValue);
						jQuery('#kapp_countdownTime').val(countdownValueTime)
						//kapp_countdownDate
						function kapp_countdownDatePicker() {

							jQuery('#kapp_countdownDate').pickadate({

								container: '#koLeftMenu',
								format: 'yyyy-mm-dd'

							});

							jQuery('#kapp_countdownTime').pickatime({

								container: '#koLeftMenu',
								format: 'H:i'
								//format: 'yyyy-mm-dd'

							});

							
						}
						// load data picker JS
						if (typeof jQuery(window).pickadate === "undefined") {

							koScriptLoader([

								'https://cdn.jsdelivr.net/npm/pickadate@3.6.4/lib/compressed/themes/default.css',
								'https://cdn.jsdelivr.net/npm/pickadate@3.6.4/lib/compressed/themes/default.date.css',
								'https://cdn.jsdelivr.net/npm/pickadate@3.6.4/lib/compressed/themes/default.time.css',
								'https://cdn.jsdelivr.net/npm/pickadate@3.6.4/lib/compressed/picker.js',
								'https://cdn.jsdelivr.net/npm/pickadate@3.6.4/lib/compressed/picker.date.js',
								'https://cdn.jsdelivr.net/npm/pickadate@3.6.4/lib/compressed/picker.time.js'
								
							], function() {

								kapp_countdownDatePicker();

							});

						} else kapp_countdownDatePicker();

						// show countdown controls
						jQuery('#kappControl_countdown').removeClass('d-none');
					} else {
					}
					koMenuOpen('widget');
					return false;

				} else if (i == 'Link') {
					//jQuery('#kToolbar').hide();
					keditableToolbar('hide');

					var lk = jQuery(inlineEditLink).parent('a');

					// verify link type, as it may be normal inline link,
					// button or koSquare_content link with title and background;
					var linkHeader = lk.find('strong'),
						linkIcon = lk.find('i'),
						linkIconSelected='';

					jQuery('#koToolbar_linkTitle,#koToolbar_linkImage,#koToolbar_linkIcon').remove();

					jQuery('#k_linkType_moreOptions,#k_linkType_moreOptionsHolder').addClass('d-none');
					jQuery('#k_linkHref,#k_linkFile').val(null);
					jQuery('#k_linkFileButton').hide();
					
				
					jQuery('#k_linkStyleHolder').after('<div id="koToolbar_linkIcon" class="mt-4">'
					
					+ '<div class="koMenuSectionTitle my-4">Link icon</div>'
					+ '<input type="hidden" class="form-control form-iconpicker" id="koToolbar_linkIconClass">'
						

						+
						'<div id="koMenuCardLinkIcon" class="koMenuCard mt-3 mb-4">'
						
						+
						
						'<div class="form-check"><input type="radio" id="k_linkIcon_1" name="k_linkIcon" value="1" class="form-check-input" checked><label class="form-check-label" for="k_linkIcon_1">None</label></div>'
						+
						
						'<div class="form-check"><input type="radio" id="k_linkIcon_2" name="k_linkIcon" value="2" class="form-check-input"><label class="form-check-label" for="k_linkIcon_2">Left</label></div>'
						

						+
						
						'<div class="form-check"><input type="radio" id="k_linkIcon_3" name="k_linkIcon" value="3" class="form-check-input"><label class="form-check-label" for="k_linkIcon_3">Right</label></div>'
						
						
						+'<div data-placement="inline" id="koToolbar_linkIconSelect" data-selected="fa-align-justify" class="mt-3 d-none"></div>'
						
						
						+'</div></div><style>.iconpicker-popover.inline{width:100%!important;background: none;border: none;}.iconpicker .iconpicker-item {width: 25px;height: 25px;font-size: 25px;}.iconpicker-popover.popover .popover-title {border: 0;border-radius: 5px;}.iconpicker .iconpicker-items {padding: 12px 0 0 7px;}</style>');
						jQuery('input[name=k_linkIcon]').on('change',function(){
//
							
							if(jQuery(this).val() > 1){

								jQuery('#koToolbar_linkIconSelect').removeClass('d-none');

								setTimeout(function(){
									
									// scroll to bottom, to see icons
									jQuery('#koMenu_link .koMenu').animate({
										scrollTop: jQuery(this).height()
									}, 200);

								},100);

								jQuery('#koToolbar_linkIcon')
									.find('.iconpicker-search')
									.focus();

							} else {

								jQuery('#koToolbar_linkIconSelect').addClass('d-none');

							}

						});

						

						if (linkIcon.length > 0) {
							
							var linkIcon=linkIcon.attr('class');
							if(typeof linkIcon!=="undefined"){

								jQuery('input[name=k_linkIcon]').prop('checked',false);
								jQuery('#koToolbar_linkIconSelect').removeClass('d-none');

								linkIcon=' '+linkIcon.replace('fa-fw','');
								linkIcon=linkIcon.split(' m');

								if(linkIcon[1].substr(0,1) == 'r' || linkIcon[1].substr(0,1) == 'e'){

									// icon is on the left side of button
									jQuery('#k_linkIcon_2').prop('checked',true);

								} else {

									// icon is on the right side.
									jQuery('#k_linkIcon_3').prop('checked',true);

								}

								if(linkIcon[0].indexOf('fa-')>-1)
									linkIconSelected=linkIcon[0];
								else if(linkIcon[1].indexOf('fa-')>-1){

									linkIconSelected=linkIcon[1].split('fas ');
									if(linkIconSelected.length>1)
									linkIconSelected='fas '+linkIconSelected[1];
									else linkIconSelected='';

								}

								linkIconSelected=$.trim(linkIconSelected);
								
								
							}
							

						}

						
						if(typeof faIcons_iconpicker==="undefined"){

							var faIcons_iconpicker=[],faIcons_tmp='';

							for (let i = 0; i < faIcons.length; i++) {
								
								faIcons_tmp='';

								if(faIcons[i].indexOf('|')){
									faIcons_tmp=faIcons[i].split('|');
									faIcons[i]=faIcons_tmp[0];
									faIcons_tmp=faIcons_tmp[1];
								}
								
								if(typeof faIcons_tmp === "undefined")
									faIcons_tmp="";

								faIcons_iconpicker.push({
									
									'title':faIcons[i],
									'searchTerms':[faIcons_tmp]

								}); 
								
							} 

						}

						// https://github.com/itsjavi/fontawesome-iconpicker

						jQuery('#koToolbar_linkIconSelect').iconpicker({
							
							title: 'Choose an Icon',
							selected:linkIconSelected,
							icons:faIcons_iconpicker,
							templates: {

        						search: '<input type="search" class="form-control iconpicker-search" style="margin:12px 0 2px" placeholder="Search..." />'
							
							}

						}).on('iconpickerShown', function(event){

							// scroll to currently selected icon
							var selectedIcon=event.iconpickerValue;
							var parentDiv=jQuery('.iconpicker-items');
							
							if(selectedIcon===null || selectedIcon.indexOf(' ') < 0 ){

								selectedIcon=parentDiv.first('.iconpicker-item');
								

							}

							selectedIcon=parentDiv.find('i[class="'+selectedIcon+'"]');
							parentDiv.scrollDivToElement(selectedIcon);
							
						});

					if (linkHeader.length > 0) {
						// koSquare_content link
						var tmpLink = lk.find('span').clone();
						tmpLink.find('strong').remove();

						var bgImage = lk.find('em').attr('style');

						if (typeof bgImage !== "undefined") {

							bgImage = bgImage.replace(/.*\s?url\([\'\"]?/, '').replace(/[\'\"]?\).*/, '');

							// add extra fields to toolbar.
							var whereToAdd = jQuery("#k_linkEditFields");
							
							// add link's title

							whereToAdd.html(

								'<div class="koMenuField" id="koToolbar_linkTitle"><input placeholder="Link Title" id="k_linkTitle" type="text" value="' + linkHeader.text().trim() + '"><label for="k_linkTitle">Link Title</label></div>'

								+
								'<div class="koMenuField"><textarea placeholder="Link Text" id="k_linkText" style="resize: none"></textarea><label for="k_linkText">Link Text</label></div>'

							)

							if (bgImage == '') bgImage = FilexLocation + 'editor_images/nophoto.png';

							// now add image selection:
							whereToAdd.after('<div class="koMenuCard my-3 koToolbar_input" id="koToolbar_linkImage" style="padding-bottom:0;min-height:80px;"><label for="k_linkImage" style="font-size:15px;color:#000"><a href="javascript:void(null)" onclick="showModalManager(\'k_linkImage\');" style="position:absolute;right:10px;top:10px;padding:0;margin:0;border:0;border-radius:10px;box-shadow:0 0 10px rgba(0,0,0,0.2)" class="jBoxTooltip btn pull-right"><img id="preview_k_linkImage" src="' + bgImage + '" class="" style="border-radius:10px;width:auto;max-width:100px;max-height:60px;height:auto;"></a>	Link Background<small style="display:block;font-size: 12px;color: rgba(0, 0, 0, 0.5);"><a href="javascript:void(null)" onclick="showModalManager(\'k_linkImage\');"><i class="fa fa-image me-1"></i>Select an image</a></small></label></div><input id="k_linkImage" value="' + bgImage + '" type="hidden"><div class="clearfix"></div></div>');

							setTimeout(function() {

								jQuery('#k_linkText').val(tmpLink.text().trim()).focus();
								jQuery('#k_linkHeader').val(linkHeader.text().trim());
								jQuery('#k_linkBackground').val(lk.find('em').attr('background-image'));

							}, 100);

							jQuery('#k_linkStyleHolder,#koToolbar_linkIcon').hide();

						}

					}

					if (typeof bgImage === "undefined") {

						jQuery('#k_linkEditFields').html(

							'<div class="koMenuField"><input placeholder="Link Text" id="k_linkText" type="text"><label for="k_linkText">Link Text</label></div>'

						)

						jQuery('#k_linkStyleHolder').show();
						var lkClass = lk.attr('class');
						if (typeof lkClass == 'undefined') lkClass = '';

						lkClass = lkClass.split(' ');
						if (lkClass.indexOf('btn') > -1) {

							//alert(lkClass.indexOf('btn-lg'))
							// button size:
							if (lkClass.indexOf('btn-xs') > -1)
								jQuery('#k_linkSize').val(1);
							else if (lkClass.indexOf('btn-sm') > -1)
								jQuery('#k_linkSize').val(2);
							else if (lkClass.indexOf('btn-lg') > -1)
								jQuery('#k_linkSize').val(4);
							else if (lkClass.indexOf('btn-xl') > -1)
								jQuery('#k_linkSize').val(5);
							else
								jQuery('#k_linkSize').val(3); // default.

							// button style:
							if (lkClass.indexOf('btn-ghost') > -1)
								jQuery('#k_linkStyle').val(11);
							else if (lkClass.indexOf('btn-secondary') > -1 || lkClass.indexOf('btn-default') > -1)
								jQuery('#k_linkStyle').val(5);
							else if (lkClass.indexOf('btn-outline-secondary') > -1)
								jQuery('#k_linkStyle').val(501);
